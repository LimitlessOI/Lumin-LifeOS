```prisma
datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model AnalyticsEvent {
  id        String   @id @default(cuid())
  type      String
  data      Json
  createdAt DateTime @default(now())
}

model AnalyticsReport {
  id          String   @id @default(cuid())
  name        String
  data        Json
  generatedAt DateTime @default(now())
}
```