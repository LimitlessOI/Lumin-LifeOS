datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model User {
  id    Int    @id @default(autoincrement())
  email String @unique
  name  String
}

model Subscription {
  id       Int    @id @default(autoincrement())
  userId   Int
  plan     String
  status   String
  user     User   @relation(fields: [userId], references: [id])
}