```prisma
datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model UserFinancialProfile {
  id        Int    @id @default(autoincrement())
  userId    String
  profile   Json
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}

model FinancialTransaction {
  id          Int    @id @default(autoincrement())
  userId      String
  amount      Float
  category    String
  date        DateTime
  description String
  createdAt   DateTime @default(now())
}

model AIInsight {
  id        Int    @id @default(autoincrement())
  userId    String
  insight   Json
  createdAt DateTime @default(now())
}

model EducationalModule {
  id          Int    @id @default(autoincrement())
  title       String
  content     String
  createdAt   DateTime @default(now())
}

model UserLearningProgress {
  id              Int    @id @default(autoincrement())
  userId          String
  moduleId        Int
  progress        Float
  completedAt     DateTime?
  createdAt       DateTime @default(now())

  module EducationalModule @relation(fields: [moduleId], references: [id])
}
```