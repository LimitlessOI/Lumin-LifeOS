```prisma
datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model SkillProfile {
  id        Int       @id @default(autoincrement())
  name      String
  userId    String
  assessments SkillAssessment[]
}

model SkillAssessment {
  id          Int       @id @default(autoincrement())
  skillId     Int
  profile     SkillProfile @relation(fields: [skillId], references: [id])
  score       Float
  evidenceUrl String?
}

model LearningPath {
  id        Int       @id @default(autoincrement())
  name      String
  description String
  skillProfiles SkillProfile[]
}

model SkillNFT {
  id        Int       @id @default(autoincrement())
  tokenId   String
  metadataUrl String
}

model GovernanceProposal {
  id        Int       @id @default(autoincrement())
  title     String
  description String
  status    String
}

model ProjectMarketplace {
  id        Int       @id @default(autoincrement())
  projectId String
  title     String
  description String
  budget    Float
}
```