```javascript
const express = require('express');
const axios = require('axios');
const { ClimateData, ClimateSensor } = require('../../models/climateModels');
const redis = require('redis');
const fetch = require('node-fetch');
const router = express.Router();

const redisClient = redis.createClient({ url: process.env.REDIS_URL });

router.post('/webhook', async (req, res) => {
  try {
    const { sensorId, temperature, humidity, timestamp } = req.body;
    const sensor = await ClimateSensor.findByPk(sensorId);
    if (!sensor) {
      return res.status(404).json({ error: 'Sensor not found' });
    }
    await ClimateData.create({ sensorId, temperature, humidity, timestamp });

    // Push to LifeOS queue
    await redisClient.lpush('climateDataQueue', JSON.stringify(req.body));

    res.status(200).json({ message: 'Data recorded successfully' });
  } catch (error) {
    console.error('Error recording data:', error);
    res.status(500).json({ error: 'Internal Server Error' });
  }
});

module.exports = router;
```